#include<iostream>
using namespace std;

int main ()
{
	
	float grade; 
	
	cout<<"Input your Grade:";
	cin>>grade;
	
	if (grade >=94 and grade <100)
	{
		cout<<"Your Grade is Excellent!";
	}
	else if (grade >=88.5 and grade <93.99)
	{
		cout<<"Your grade is Superior!";
	}
	else if (grade >=83 and grade <88.49)
	{
		cout<<"Your grade is Mertorious!";
	}
	else if (grade >=77.5 and grade <82.99)
	{
		cout<<"Your grade is Very Good!";
	}
	else if (grade >=72 and grade <77.49)
	{
		cout<<"Your grade is Good!";
	}
	else if (grade >=65.5 and grade <71.99)
	{
		cout<<"Your grade is Very Satisfactory!";
	}
	else if (grade >=61 and grade <65.49)
	{
		cout<<"Your grade is Satisfactory!";
	}
	else if (grade >=55.5 and grade <60.99)
	{
		cout<<"Your grade is Fair!";
	}
	else if (grade >=50 and grade <55.49)
	{
		cout<<"Your grade is Passing!";
	}
	else if (grade >=50 and grade <55.49)
	{
		cout<<"Your grade is Incomplete!";
	}
	else if (grade >=0 and grade <49.99)
	{
		cout<<"Your grade is Failed, HAHAHA  ";
	}
	else if (grade > 100 or grade <0)
	{
		cout<<"ERROR";
	}
		return 0;
}
